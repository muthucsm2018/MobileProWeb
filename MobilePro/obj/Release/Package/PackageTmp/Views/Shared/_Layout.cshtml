<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Mobile Pro</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    @Styles.Render("~/assets/css/css")

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->  
     @Scripts.Render("~/assetsjs")
    <!-- Datatable js -->
    @Scripts.Render("~/Datatablesjs")

    <!-- Full Calendar js -->
    @Scripts.Render("~/FullCalendarjs")

    <link href="@Url.Content("~/assets/plugins/fullcalendar/fullcalendar.print.css")" rel="stylesheet" media='print' />

    @*<script src="@Url.Content("~/assets/plugins/fullcalendar/moment.min.js")"></script>
    <script src="@Url.Content("~/assets/plugins/fullcalendar/fullcalendar.js")"></script>
   
    <link href="@Url.Content("~/assets/plugins/fullcalendar/fullcalendar.css")" rel="stylesheet" />
    <link href="@Url.Content("~/assets/plugins/fullcalendar/fullcalendar.print.css")" rel="stylesheet" media='print' />*@
    
    <script type="text/javascript">

    var isShift = false;

    $(document).ready(function() {

	    // action on key up
	    $(document).keyup(function(e) {
		    if(e.which == 16) {
			    isShift = false;
		    }
	    });
	    

    });
    </script>
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper" >
        <header class="main-header">
            <!-- Logo -->
            
            <nav class="navbar navbar-fixed-top" >
                <div class="navbar-header">

                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
                        <i class="fa fa-bars"></i>
                    </button>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse text-center" id="navbar-collapse">
                    <ul class="nav navbar-nav">
                        <!-- User Account Menu -->
                       @Html.Partial("_LoginPartial")

                       @{ Html.RenderAction("Menu", "Home"); }  

                    </ul>
                </div><!-- /.navbar-collapse -->

            </nav>
           
        </header>       
       
        
        <!-- Content Wrapper. Contains page content -->
        @RenderBody()
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0.1
            </div>
            <strong>Copyright &copy; @DateTime.Now.Year <a href="http://Cloudin.in">Cloudin Technology</a>.</strong> All rights reserved.
        </footer>
    </div>
    <!-- ./wrapper -->

   
    @RenderSection("scripts", required: false)
</body>

</html>
